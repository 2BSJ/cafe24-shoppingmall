<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="product">

	<insert id="insertProduct" parameterType='categoryvo'>
	<![CDATA[
	INSERT
	INTO product
	VALUES(null,
		   #{name},
		   #{simpleDescription},
		   #{description},
		   #{manufacture},
		   0,
		   #{price},
		   #{deliverycost},
		   #{displayStatus},
		   #{salesStatus},
		   #{specialStatus},
		   #{titleStatus},
		   #{couponStatus},
		   #{categoryNo}
		   )
	]]>
	</insert>	

	
</mapper>